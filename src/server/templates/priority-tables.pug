each name, index in priorityNames
  div
    h5
      b= `Предлагаемые размещения (Приоритет ${index + 1}), ${placesByPriority[name].length} шт`
    table(class=`table table-light`)
      tr
        th #
        th Наименование
        th Адрес
      each item, index in placesByPriority[name]
        tr
          td= index + 1
          td= item[`name`]
          td= item[`address`]
